        "{{PROJECT_PATH}}": {
          "commands": [
            { "path": ".claude/commands/configure-root-artifacts.md", "hash": "{{HASH}}" }
          ],
          "tools": [
            { "path": ".dev-pomogator/tools/forbid-root-artifacts/check.py", "hash": "{{HASH}}" },
            { "path": ".dev-pomogator/tools/forbid-root-artifacts/setup.py", "hash": "{{HASH}}" }
          ]
        }
      }
    },
    {
      "name": "auto-commit",
      "version": "2.1.0",
      "platform": "claude",
      "projectPaths": ["{{PROJECT_PATH}}"],
      "managed": {
        "{{PROJECT_PATH}}": {
          "tools": [
            { "path": ".dev-pomogator/tools/auto-commit/auto_commit_stop.ts", "hash": "{{HASH}}" }
          ],
          "hooks": {
            "Stop": [
              "npx tsx .dev-pomogator/tools/auto-commit/auto_commit_stop.ts"
            ]
          }
        }
      }
